{
  "name": "webcomponents",
  "private": true,
  "version": "2.1.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "build:wc": "vite build --config vite.wc.config.ts",
    "release:patch": "npm run build:wc && node scripts/publish.js patch",
    "release:minor": "npm run build:wc && node scripts/publish.js minor",
    "release:major": "npm run build:wc && node scripts/publish.js major",
    "publish:npm": "npm run release:patch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint \"**/*.{css,scss}\"",
    "lint:style:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "lint:all": "npm run lint && npm run lint:style",
    "preview": "vite preview"
  },
  "dependencies": {
    "@carbon/react": "^1.97.0",
    "@carbon/styles": "^1.96.0",
    "@r2wc/react-to-web-component": "^2.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "sass": "^1.97.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.13.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}